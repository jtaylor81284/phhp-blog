@article{masonWhereWhomDoes2020,
  title = {Where and for Whom Does the Neighbourhood Built Environment Matter for Obesity and Health? {{Examining}} Sources of Effect Heterogeneity at Multiple Scales in the {{UK}} Adult Population},
