  number = {3}
}

@article{weygandtImpulseControlDorsolateral2015,
  title = {Impulse Control in the Dorsolateral Prefrontal Cortex Counteracts Post-Diet Weight Regain in Obesity},
  author = {Weygandt, Martin and Mai, Knut and Dommes, Esther and Ritter, Kerstin and Leupelt, Verena and Spranger, Joachim and Haynes, John-Dylan},
